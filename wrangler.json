{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "facets",
	"main": "./api/index.ts",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": ["nodejs_compat"],
	"routes": [
		{
		"pattern": "facets.stephanofer.com",
		"custom_domain": true
		}
	],
	"observability": {
		"enabled": true
	},
	"upload_source_maps": true,
	"assets": {
		"directory": "./dist/client",
		"not_found_handling": "single-page-application"
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "facets-db",
			"database_id": "8873de7f-52a8-4d0c-bb9b-7f57207181ab"
		}
	],
	"r2_buckets": [
		{
			"binding": "STORAGE",
			"bucket_name": "facets-storage"
		}
	],
	"ai": {
		"binding": "AI"
	}
}
